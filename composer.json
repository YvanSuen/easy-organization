{
    "name": "jucloud/easy-organization",
    "description": "这是我使用过最优雅的方式查询企业工商信息的SDK了",
    "keywords": ["tianyancha", "qichacha", "qixin", "organization"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jucloud/easy-organization/issues",
        "source": "https://github.com/jucloud/easy-organization",
        "homepage": "https://github.com/jucloud"
    },
    "authors": [
        {
            "name": "jucloud",
            "email": "support@jujiang.biz"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "ext-libxml": "*",
        "ext-json": "*",
        "ext-bcmath": "*",
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "psr/container": "^1.1 || ^2.0",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0.0",
        "monolog/monolog": "^2.2",
        "symfony/var-dumper": "^5.1",
        "symfony/http-foundation": "^5.2.0",
        "symfony/event-dispatcher": "^5.2.0",
        "symfony/psr-http-message-bridge": "~2.0|^2.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "JuCloud\\EasyOrganization\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JuCloud\\EasyOrganization\\Tests\\": "tests"
        }
    },
    "suggest": {
        "illuminate/container": "Laravel 框架下使用 SDK，请安装",
        "hyperf/pimple": "其它/无框架下使用 SDK，请安装"
    }
}
